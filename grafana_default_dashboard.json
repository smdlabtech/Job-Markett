          "rawSql": "SELECT \n  title,\n  COUNT(*) AS offer_count\nFROM (\n  -- France Travail\n  SELECT france_travail_offers.title\n  FROM france_travail_offers\n  JOIN job_offers ON france_travail_offers.job_id = job_offers.job_id\n  WHERE \n    job_offers.status = 'active'\n    AND france_travail_offers.title IS NOT NULL\n    AND TRIM(france_travail_offers.title) <> ''\n    AND (\n      france_travail_offers.title ILIKE '%data%' OR\n      france_travail_offers.title ILIKE '%cyber%' OR\n      france_travail_offers.title ILIKE '%dÃ©veloppeur%' OR\n      france_travail_offers.title ILIKE '%developpeur%' OR\n      france_travail_offers.title ILIKE '%cloud%' OR\n      france_travail_offers.title ILIKE '%ingÃ©nieur%' OR\n      france_travail_offers.title ILIKE '%ingenieur%' OR\n      france_travail_offers.title ILIKE '%software%' OR\n      france_travail_offers.title ILIKE '%numÃ©rique%' OR\n      france_travail_offers.title ILIKE '%digital%'\n    )\n\n  UNION ALL\n\n  -- Adzuna\n  SELECT adzuna_offers.title\n  FROM adzuna_offers\n  JOIN job_offers ON adzuna_offers.job_id = job_offers.job_id\n  WHERE \n    job_offers.status = 'active'\n    AND adzuna_offers.title IS NOT NULL\n    AND TRIM(adzuna_offers.title) <> ''\n    AND (\n      adzuna_offers.title ILIKE '%data%' OR\n      adzuna_offers.title ILIKE '%cyber%' OR\n      adzuna_offers.title ILIKE '%dÃ©veloppeur%' OR\n      adzuna_offers.title ILIKE '%developpeur%' OR\n      adzuna_offers.title ILIKE '%cloud%' OR\n      adzuna_offers.title ILIKE '%ingÃ©nieur%' OR\n      adzuna_offers.title ILIKE '%ingenieur%' OR\n      adzuna_offers.title ILIKE '%software%' OR\n      adzuna_offers.title ILIKE '%numÃ©rique%' OR\n      adzuna_offers.title ILIKE '%digital%'\n    )\n\n  UNION ALL\n\n  -- JSearch\n  SELECT jsearch_offers.title\n  FROM jsearch_offers\n  JOIN job_offers ON jsearch_offers.job_id = job_offers.job_id\n  WHERE \n    job_offers.status = 'active'\n    AND jsearch_offers.title IS NOT NULL\n    AND TRIM(jsearch_offers.title) <> ''\n    AND (\n      jsearch_offers.title ILIKE '%data%' OR\n      jsearch_offers.title ILIKE '%cyber%' OR\n      jsearch_offers.title ILIKE '%dÃ©veloppeur%' OR\n      jsearch_offers.title ILIKE '%developpeur%' OR\n      jsearch_offers.title ILIKE '%cloud%' OR\n      jsearch_offers.title ILIKE '%ingÃ©nieur%' OR\n      jsearch_offers.title ILIKE '%ingenieur%' OR\n      jsearch_offers.title ILIKE '%software%' OR \n      jsearch_offers.title ILIKE '%numÃ©rique%' OR \n      jsearch_offers.title ILIKE '%digital%' \n    )\n) AS combined_titles\nGROUP BY title\nORDER BY offer_count DESC\nLIMIT 15;",
